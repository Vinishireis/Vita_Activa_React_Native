{
  "expo": {
    "name": "Vita_Activa",
    "slug": "Vita_Activa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/VitaActiva_SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#2260FF",
      "updates": {
        "enabled": true,
        "checkAutomatically": "ON_LOAD",
        "fallbackToCacheTimeout": 0
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Adaptive-icon.png",
        "backgroundColor": "#2260FF"
      },
      "package": "com.vinishireis.Vita_Activa"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/VitaActiva_Favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c1791b69-4d61-4db7-b90d-248ae36b742c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c1791b69-4d61-4db7-b90d-248ae36b742c"
    }
  }
}
